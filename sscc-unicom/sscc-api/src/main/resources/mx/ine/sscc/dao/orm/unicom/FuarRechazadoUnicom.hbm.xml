<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="mx.ine.sscc.modelo.entidad.unicom"
	schema="CREDENCIALEXTRANJERO">
	<class name="FuarRechazadoUnicom" table="FUAR_RECHAZADO">
		<comment>Mapeo de la tabla FUARRECHAZADO (CREDENCIALEXTRANJERO)</comment>

		<id name="fuar">
			<column name="FUAR" />
		</id>
		<property name="fechaRechazoSi" type="java.sql.Timestamp">
			<column name="FECHA_RECHAZO_SI" sql-type="DATE" />
		</property>
		<property name="clave">
			<column name="CLAVE"/>
		</property>
		<property name="descripcion">
			<column name="DESCRIPCION" />
		</property>
		<property name="procesoSincronizacionId">
			<column name="PROCESOSINCRONIZACION_ID" />
		</property>		

	</class>
</hibernate-mapping>